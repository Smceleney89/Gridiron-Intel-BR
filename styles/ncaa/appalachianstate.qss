    max-width: 260px;
    min-width: 200px;
}

QComboBox {
    max-width: 250px;
    min-width: 200px;
    padding: 5px;
    border-style: outset;
    border-width: 2px;
    border-radius: 5px;
    border-color: #FFCC00; /*primary*/
    background-color: white;
}

#route {
    max-width: 700px;
    margin: 0px;
    spacing: 0px;
}
