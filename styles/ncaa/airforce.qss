#questions {
    max-width: 350px;
    min-width: 300px;
    max-height: 120px;
}

#push {
    color: white;
    background-color: #003087; /*primary*/
    border :3px outset;
    border-color: #828A8F;
    border-radius: 5px;
    max-width: 260px;
    min-width: 200px;
}

QComboBox {
    max-width: 250px;
    min-width: 200px;
    padding: 5px;
    border-style: outset;
    border-width: 2px;
    border-radius: 5px;
    border-color: #003087; /*primary*/
}

#route {
    max-width: 700px;
    margin: 0px;
    spacing: 0px;
}
#AirForceoffCombo {
    max-width: 150px;
    min-width: 150px;
    background-color: #003087;
    color: white;
}

#AirForceoffquestions {
    max-width: 160px;
    min-width: 160px;
    background-color: #003087;
    color: white;
}

#AirForceoffname {
    background-color: #003087;
    color: white;
}

#AirForcedefcombo {
    max-width: 150px;
    min-width: 150px;
    background-color: #003087;
    color: white;
}

#AirForcedefquestions {
    max-width: 160px;
    min-width: 160px;
    background-color: #003087;
    color: white;
}

#AirForcedefname {
    background-color: #003087;
    color: white;
}

#AirForcedeflabel {
    max-width: 75px;
    min-width: 75px;
    background-color: #003087;
    color: white;
}

#AirForceofflabel {
    max-width: 75px;
    min-width: 75px;
    background-color: #003087;
    color: white;
}

#AirForcetable {
    alternate-background-color: #5E6A71;
    selection-background-color: #003087; /*primary*/
}


QComboBox::drop-down, #endCombo::drop-down {
    border: 0px; /* This seems to replace the whole arrow of the combo box */
}

